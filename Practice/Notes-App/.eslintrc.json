{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "linebreak-style": "off",
        "no-multiple-empty-lines": ["error", 1]
    }
}
